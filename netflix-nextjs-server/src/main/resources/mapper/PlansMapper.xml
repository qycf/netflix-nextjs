<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qu2u.mapper.PlansMapper">

    <resultMap id="BaseResultMap" type="com.qu2u.domain.Plans">
            <id property="planId" column="plan_id" jdbcType="INTEGER"/>
            <result property="planName" column="plan_name" jdbcType="VARCHAR"/>
            <result property="planDurationDays" column="plan_duration_days" jdbcType="INTEGER"/>
            <result property="planDescription" column="plan_description" jdbcType="VARCHAR"/>
            <result property="planPrice" column="plan_price" jdbcType="DECIMAL"/>
            <result property="planStatus" column="plan_status" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        plan_id,plan_name,plan_duration_days,
        plan_description,plan_price,plan_status
    </sql>
</mapper>
